{
  "name": "cucumber-statistics",
  "version": "0.0.10",
  "description": "A cucumber standalone tests based statistics server.",
  "main": "main.js",
  "scripts": {
    "install": "cd public && bower install && cd ..",
    "test": "node ./.scripts/test.js",
    "toTest": "node ./.scripts/toTest.js",
    "testServer": "cd acceptanceTests && node testServer && cd ..",
    "utest": "./node_modules/.bin/jasmine-node --color ./test",
    "autotest": "./node_modules/.bin/jasmine-node --autotest --color ./test",
    "start": "node main -m"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danyg/cucumber-statistics.git"
  },
  "keywords": [
    "cucumber",
    "statistics"
  ],
  "author": "danyg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danyg/cucumber-statistics/issues"
  },
  "dependencies": {
    "body-parser": "~1.14.1",
    "commander": "^2.11.0",
    "connect-gzip-static": "git+https://github.com/danyg/connect-gzip-static.git",
    "express": "~4.13.3",
    "express-logger": "0.0.3",
    "mkpath": "~1.0.0",
    "mongodb": "latest",
    "nedb": "~1.7.4",
    "q": "~1.4.1",
    "sanitize-filename": "~1.5.3",
    "ws": "^3.1.0"
  }
}
