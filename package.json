{
  "name": "cucumber-statistics",
  "version": "0.0.0",
  "description": "A cucumber standalone tests based statistics server.",
  "main": "main.js",
  "scripts": {
    "install": "cd public && bower install && cd ..",
    "test": "./node_modules/.bin/jasmine-node --color ./test",
    "autotest": "./node_modules/.bin/jasmine-node --autotest --color ./test",
    "start": "node main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danyg/cucumber-statistics.git"
  },
  "keywords": [
    "cucumber",
    "statistics"
  ],
  "author": "danyg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danyg/cucumber-statistics/issues"
  },
  "dependencies": {
    "express": "~4.13.3",
    "nedb": "~1.7.4",
    "body-parser": "~1.14.1",
    "sanitize-filename": "~1.5.3",
    "q": "~1.4.1",
    "connect-gzip-static": "git+https://github.com/danyg/connect-gzip-static.git",
    "mkpath": "~1.0.0"
  }
}
